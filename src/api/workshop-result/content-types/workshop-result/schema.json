{
  "kind": "collectionType",
  "collectionName": "workshop_results",
  "info": {
    "singularName": "workshop-result",
    "pluralName": "workshop-results",
    "displayName": "Workshop Result",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "evaluator": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "evaluationStatus": {
      "type": "enumeration",
      "enum": [
        "done",
        "inProgress",
        "todo"
      ],
      "default": "to do"
    },
    "evaluation_step": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::evaluation-step.evaluation-step"
    },
    "Result": {
      "type": "dynamiczone",
      "components": [
        "media.totality",
				"media.research"
      ]
    },
    "workshop": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::workshop.workshop",
      "inversedBy": "workshop_results"
    },
    "estimatedCompletion": {
      "type": "date"
    }
  }
}
